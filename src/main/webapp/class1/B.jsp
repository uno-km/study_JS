<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<script type="text/javascript">
		alert("제곱 : " + Math.pow((1 + 1) * 4, 3));
		alert("반올림 : "+Math.round(10.6)); //반올림
		alert("올림 : "+Math.ceil(10.2)); //올림
		alert("내림 : "+Math.floor(10.6)); //내림
		alert("제곱근 : "+Math.sqrt(9)); //제곱근
		alert("무작위값 : "+Math.random() * 10); //랜덤값
		alert("무작위값 소수 반올림 : "+Math.round(Math.random() * 10)); //랜덤값
	</script>
</body>
</html>